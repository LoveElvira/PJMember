<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/item_dispatch__parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/size_10"
    android:layout_marginLeft="@dimen/size_12"
    android:layout_marginRight="@dimen/size_12"
    android:background="@drawable/bg_rectangle_white_radius_3"
    android:orientation="vertical"
    android:padding="@dimen/size_15">

    <TextView
        android:id="@+id/item_dispatch__title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/gray_888888"
        android:textSize="@dimen/text_13" />


    <TextView
        android:id="@+id/item_dispatch__content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_10"
        android:textColor="@color/gray_ADADAD"
        android:textSize="@dimen/text_12" />

</LinearLayout>